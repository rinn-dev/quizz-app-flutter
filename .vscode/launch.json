  "version": "0.2.0",
  "configurations": [
    {
      "name": "quizz_app",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-hostname",
        "localhost",
        "--web-port",
        "5000",
        "--web-renderer",
        "html",
        "--device-id",
        "chrome"
      ]
    },
    {
      "name": "quizz_app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "quizz_app (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
